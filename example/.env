# .env.example

# String (no quotes)
APP_NAME=envpoint_dev

# String (single quotes)
APP_TITLE='EnvPoint Example App'

# String (double quotes)
APP_DESCRIPTION="This is an example .env file"

# Number (no quotes)
APP_PORT=8080

# Boolean (no quotes)
APP_DEBUG=true
APP_PRODUCTION=false

# Boolean (as string)
APP_ENABLED="true"
APP_DISABLED='false'

# Array (comma-separated, no quotes)
ALLOWED_HOSTS=localhost,127.0.0.1,example.com

# Array (comma-separated, with quotes)
ALLOWED_ORIGINS="http://localhost:3000","https://example.com"

# Null/empty value
OPTIONAL_VALUE=

# Export syntax (for shell environments)
export DB_HOST=localhost
export DB_PORT=5432
export DB_USER='user'
export DB_PASS="password"

# Special characters
PASSWORD="p@ssw0rd!$"
SECRET_KEY='s3cr3t!@#'

# Multiline value (not officially supported, but some parsers allow it)
MULTILINE="Line1
Line2
Line3"

# Comments are ignored

# Numeric string
VERSION="1.0.0"

# Quoted number (as string)
MAX_USERS="100"

# Unquoted number
MAX_CONNECTIONS=100

# Boolean (capitalized)
IS_ACTIVE=TRUE
IS_BLOCKED=FALSE

# Single character
FLAG=Y

# JSON-like string
CONFIG_JSON='{"key":"value","flag":true}'

# End of file